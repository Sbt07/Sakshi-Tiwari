def calculate_profit(units_sold, selling_price_per_unit, NRE_cost, unit_cost):
    revenue = units_sold * selling_price_per_unit
    total_cost = NRE_cost + (units_sold * unit_cost)
    profit = revenue - total_cost
    return profit

# Example values
units_sold = 1000
selling_price_per_unit = 200
NRE_cost = 2000
unit_cost = 100

profit = calculate_profit(units_sold, selling_price_per_unit, NRE_cost, unit_cost)
print("Profit:", profit)
